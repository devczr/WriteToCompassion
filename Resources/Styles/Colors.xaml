<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#fdf984</Color>
    <Color x:Key="PrimaryLight">#ffffb6</Color>
    <Color x:Key="PrimaryDark">#c8c654</Color>
    <Color x:Key="Secondary">#ffea20</Color>
    <Color x:Key="SecondaryLight">#ffff60</Color>
    <Color x:Key="SecondaryDark">#c8b800</Color>

    <Color x:Key="PrimaryTextColor">#000000</Color>

    <Color x:Key="LightGray">#e5e2da</Color>
    <Color x:Key="MidGray">#e1e2e1</Color>
    <Color x:Key="DarkGray">#e5e2da</Color>

    <Color x:Key="light_primary">#5A6400</Color>
    <Color x:Key="light_onPrimary">#FFFFFF</Color>
    <Color x:Key="light_primaryContainer">#DEEB77</Color>
    <Color x:Key="light_onPrimaryContainer">#1A1E00</Color>
    <Color x:Key="light_secondary">#5E6044</Color>
